<div class="main">
  <p class="sign" align="center">新規登録</p>   
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render 'devise/shared/error_messages', model: f.object %>
    <form class="form1">
      <%= f.text_field :nickname, class:"un", autofocus: true,placeholder:"Nickname" %>
      <%= f.email_field :email, class:"un", autofocus: true, autocomplete: "email",placeholder:"Email" %>
      <%= f.password_field :password, class:"pass", autocomplete: "current-password",placeholder:"Password"  %>
      <%= f.password_field :password_confirmation, class:"pass", autocomplete: "new-password",placeholder:"Password_confirmation" %>
      <%= f.submit "Sign up", class:"submit" %>
  <% end %>
</div>
